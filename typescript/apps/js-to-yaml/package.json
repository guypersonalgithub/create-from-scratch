  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "@packages/dynatic-css-vite-plugin": "*"
