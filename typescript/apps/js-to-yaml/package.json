{
  "name": "js-to-yaml",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "set NODE_OPTIONS=--import tsx && vite --host",
    "dev-dynatic": "set NODE_OPTIONS=--import tsx && vite --host -- --dynatic-debug",
    "build": "set NODE_OPTIONS=--import tsx && vite build",
    "build-ga": "NODE_OPTIONS=--import=tsx vite build",
    "lint": "eslint .",
    "preview": "set NODE_OPTIONS=--import tsx && vite preview",
    "debug": "set NODE_OPTIONS=--import tsx && vite --debug",
    "typecheck": "tsc --noEmit -p ."
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@packages/router": "*",
