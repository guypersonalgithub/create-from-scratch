    "@packages/button": "*",
    "@packages/syntax-highlighter": "*",
    "@packages/yaml": "*",
    "@packages/tooltip": "*",
    "@packages/alert": "*",
    "@packages/card": "*",
    "@packages/command-box": "*",
    "@packages/object-utils": "*",
    "@packages/css-utils": "*",
    "@packages/breakpoints": "*",
    "@packages/animation-container": "*",
    "@packages/textarea": "*",
    "@packages/sidebar": "*",
    "@packages/element-utils": "*",
    "@packages/date": "*",
    "@packages/modal": "*",
    "@packages/badge": "*",
    "@packages/input": "*",
    "@packages/title": "*",
    "@packages/scrollspy-anchors": "*",
    "@packages/list": "*",
